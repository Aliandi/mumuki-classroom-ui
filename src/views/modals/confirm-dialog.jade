.container-fluid
  .row
    .col-sm-12
      h4.modal-title {{ title }}
    .col-sm-12
      p {{ text }}
    .col-sm-12
      button.btn.btn-default(type='button', ng-click='no()', data-dismiss='modal') {{ 'no' | translate }}
      button.btn.btn-success(type='button', ng-click='yes()') {{ 'yes' | translate }}
